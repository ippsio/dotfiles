{
  "title": "[10:COMMAND] behaviors",
  "rules": [
    {
      "description": "[10:COMMAND]: + [space/ hjkl/ ae/ d]",
      "manipulators": [
        {
          "from": {"key_code": "h", "modifiers": {"mandatory": ["command"], "optional": ["any"]}},
          "to": [{"key_code": "left_arrow"}],
          "type": "basic"
        },{
          "from": {"key_code": "j", "modifiers": {"mandatory": ["command"], "optional": ["any"]}},
          "to": [{"key_code": "down_arrow"}],
          "type": "basic"
        },{
          "from": {"key_code": "k", "modifiers": {"mandatory": ["command"], "optional": ["any"]}},
          "to": [{"key_code": "up_arrow"}],
          "type": "basic"
        },{
          "from": {"key_code": "l", "modifiers": {"mandatory": ["command"], "optional": ["any"]}},
          "to": [{"key_code": "right_arrow"}],
          "type": "basic"
        },{
        },{
          "from": {"key_code": "a","modifiers": {"mandatory": ["command"], "optional": ["any"]}},
          "to": [{"key_code": "a", "modifiers":["control"]}],
          "type": "basic"
        },{
          "from": {"key_code": "e","modifiers": {"mandatory": ["command"], "optional": ["any"]}},
          "to": [{"key_code": "e", "modifiers":["control"]}],
          "type": "basic"
        },{
          "from": {"key_code": "d","modifiers": {"mandatory": ["command"], "optional": ["any"]}},
          "to": [{"key_code": "d", "modifiers":["control"]}],
          "type": "basic"
        }
      ]
    }
  ]
}

