          "type": "basic"
        },{
          "from": {"key_code": "d","modifiers": {"mandatory": ["fn"], "optional": ["any"]}},
          "to": [{"key_code": "delete_forward"}],
          "type": "basic"
        },{
          "from": {"key_code": "tab","modifiers": {"mandatory": ["fn"], "optional": ["shift"]}},
          "to": [{"key_code": "tab", "modifiers": ["command"]}],
          "type": "basic"
        },{
          "from": {"key_code": "c", "modifiers": {"mandatory": ["fn"], "optional": ["any"]}},
