{
  "segments": [
    "time",
    {"type": "stdout","command": ["pwd"], "fg_color": 255},
    "git",
    {"type": "stdout","command": ["git-url"], "fg_color": 255},
    "newline",
    "jobs",
    "exit_code",
    "virtual_env",
    "root",
    "set_term_title"
  ],
  "mode":"flat",
  "cwd": {
    "max_depth": 5
  },
  "time": {
    "format": "%Y/%m/%d %H:%M:%S"
  },
  "vcs": {
    "show_symbol": false
  }
}

