#!/bin/bash
FILE=~/.compl_candidates/goto
COLOR=170
idx=1
while read line; do
  if [[ "${line:0:1}" == "-" ]]; then
    echo "[48;5;${COLOR}m${line:1}[0m"
  else
    echo "(${idx}) ${line}"
    idx=$(( ${idx} + 1 ))
  fi
done < ${FILE}| fzf --info=inline --prompt="goto " --ansi| grep -E "^\([0-9]+\)"| sed -E "s/^\([0-9]+\) //"
